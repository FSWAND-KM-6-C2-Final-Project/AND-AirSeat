<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_not_background"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_not_background"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".presentation.seatbookreturn.SeatBookReturnActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.detailhistory.DetailHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.resultsearchreturn.ResultSearchReturnActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.deleteaccount.DeleteAccountFragment"
            android:exported="false" />
        <activity
            android:name=".presentation.profilesetting.ProfileSettingActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.payment.PaymentSuccessActivity"
            android:exported="false"
            tools:ignore="AppLinkUrlError">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="https" android:host="airseat.netlify.app" android:pathPrefix="/payment-success" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.splashscreen.SplashScreenActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.updateprofile.UpdateProfileActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.resetpassword.ResetPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.resetpasswordverifyemail.ReqChangePasswordActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.otpresetpassword.OtpResetPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.otpaccount.OtpActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.register.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.login.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".presentation.detailnotification.DetailNotificationActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.detailflight.DetailFlightActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.resultsearch.ResultSearchActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.payment.WebViewMidtransActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.seatbook.SeatBookActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.order.OrderInfoActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.flightdetail.FlightDetailPriceActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.biodata.PassengerBioActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.biodata.OrdererBioActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.main.MainActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.appintro.AppIntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>